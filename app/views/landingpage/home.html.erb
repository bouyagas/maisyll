
<body>

    <!-- Navigation -->
    <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
        <div class=".container-fluid">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" <%= link_to "Maisyll", root_path %> 
            </div>
            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav pull-right">
                    <li>
                        <%= link_to "Log in", login_path %>
                    </li>
                    <li>
                        <%= link_to "Sign up", signup_path %>
                    </li>
                </ul>
            </div>
            <!-- /.navbar-collapse -->
        </div>
        <!-- /.container -->
    </nav>

    <!-- Full Page Image Background Carousel Header -->

    <header id="myCarousel" class="carousel slide">
        
    
    <!-- Wrapper for Slides -->
        <div class="carousel-inner">
            <div class="item active">
                <!-- Set the first background image using inline CSS below. -->
                <div class="fill" style="background-image:url('assets/north america culture.jpg');"></div>
                <div class="carousel-caption">
               <h1>Welcome to Maisyll</h1>
                    <h3>Empower your culture today</h3>
                    <h2>Nouth American Culture</h2>
                </div>
            </div>
            <div class="item">
                <!-- Set the second background image using inline CSS below. -->
                <div class="fill" style="background-image:url('assets/south american culture.jpg');"></div>
                <div class="carousel-caption">
                    <h2>South America Culture</h2>
                </div>
            </div>
            <div class="item">
                <!-- Set the third background image using inline CSS below. -->
                <div class="fill" style="background-image:url('assets/european culture.jpg');"></div>
                <div class="carousel-caption">
                    <h2>European Culture</h2>
                </div>
            </div>
             <div class="item">
                <!-- Set the third background image using inline CSS below. -->
                <div class="fill" style="background-image:url('assets/middle eastern culture.jpg');"></div>
                <div class="carousel-caption">
                    <h2>Middle Eastern Culture</h2>
                </div>
            </div>
             <div class="item">
                <!-- Set the third background image using inline CSS below. -->
                <div class="fill" style="background-image:url('assets/africanpic.jpg');"></div>
                <div class="carousel-caption">
                    <h2>Africa Culture</h2>
                </div>
            </div>
             <div class="item">
                <!-- Set the third background image using inline CSS below. -->
                <div class="fill" style="background-image:url('assets/asian culture.jpg');"></div>
                <div class="carousel-caption">
                    <h2>Asian Culture</h2>
                </div>
            </div>
             <div class="item">
                <!-- Set the third background image using inline CSS below. -->
                <div class="fill" style="background-image:url('assets/australia culture.jpg');"></div>
                <div class="carousel-caption">
                    <h2>Australian Culture</h2>
                </div>
            </div>
        </div>
        
    </header>

    

        

        
    
    <!-- jQuery -->
    <script src="js/jquery.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="js/bootstrap.min.js"></script>

    <!-- Script to Activate the Carousel -->
    <script>
    $('.carousel').carousel({
        interval: 3000 //changes the speed
    })
    </script>

</body>
