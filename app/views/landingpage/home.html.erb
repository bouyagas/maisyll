
<body>

    <!-- Navigation -->
    <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
        <div class=".container-fluid">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <a class="navbar-brand" <%= link_to "Maisyll", root_path %> 
            </div>
            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav pull-right">
                    <li>
                        <%= link_to "Log in", login_path %>
                    </li>
                    <li>
                        <%= link_to "Sign up", signup_path %>
                    </li>
                </ul>
            </div>

            <!-- /.navbar-collapse -->
        </div>
        <!-- /.container -->
    </nav>

    <!-- Full Page Image Background Carousel Header -->

    <header id="myCarousel" class="carousel slide">
        
    
    <!-- Wrapper for Slides -->
        <div class="carousel-inner">
            <div class="item active">
                <!-- Set the first background image using inline CSS below. -->
                <div class="fill" style="background-image:url('assets/north america culture.jpg');"></div>
                <div class="carousel-caption">
               <h1 class="ba">Welcome to Maisyll</h1>
                    <p class="ab">Empower your culture today throughtout the world !</p>
                    <h2>Nouth American Culture</h2>
                </div>
            </div>
            <div class="item">
                <!-- Set the second background image using inline CSS below. -->
                <div class="fill" style="background-image:url('assets/south american culture.jpg');"></div>
                <div class="carousel-caption">
                    <h2>South America Culture</h2>
                </div>
            </div>
            <div class="item">
                <!-- Set the third background image using inline CSS below. -->
                <div class="fill" style="background-image:url('assets/european culture.jpg');"></div>
                <div class="carousel-caption">
                    <h2>European Culture</h2>
                </div>
            </div>
             <div class="item">
                <!-- Set the fourth background image using inline CSS below. -->
                <div class="fill" style="background-image:url('assets/middle eastern culture.jpg');"></div>
                <div class="carousel-caption">
                    <h2>Middle Eastern Culture</h2>
                </div>
            </div>
             <div class="item">
                <!-- Set the fifth background image using inline CSS below. -->
                <div class="fill" style="background-image:url('assets/africanpic.jpg');"></div>
                <div class="carousel-caption">
                    <h2>Africa Culture</h2>
                </div>
            </div>
             <div class="item">
                <!-- Set the sixth background image using inline CSS below. -->
                <div class="fill" style="background-image:url('assets/asian culture.jpg');"></div>
                <div class="carousel-caption">
                    <h2>Asian Culture</h2>
                </div>
            </div>
             <div class="item">
                <!-- Set the seventh background image using inline CSS below. -->
                <div class="fill" style="background-image:url('assets/australia culture.jpg');"></div>
                <div class="carousel-caption">
                    <h2>Australian Culture</h2>
                </div>
            </div>
        </div>
        
    </header>

    <div class="container">

        <div class="row">
         <ul class="nav navbar-nav">
             <li>
                 <%= link_to "About", about_path %>
             </li>
             <li>
                <%= link_to "Blog", blog_path %>
             </li>
             <li>
                <%= link_to "Contact", contact_path %>
             </li>
             <li>
                <%= link_to "Term", term_path %>
             </li>
             <li>
                <%= link_to "Privacy", privacy_path %>
             </li>
             <li>
              <%= link_to "Support" , support_path %>
             </li>
        </ul> 
        </div>
        
        <!-- Footer -->
        <footer>
            <div class="row">
                <div class="col-lg-12">
                    <p>Copyright &copy; Maisyll,inc 2014</p>
                </div>
            </div>
            <!-- /.row -->
        </footer>

    </div>

        

        
    
    <!-- jQuery -->
    <script src="js/jquery.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="js/bootstrap.min.js"></script>

    <!-- Script to Activate the Carousel -->
    <script>
    $('.carousel').carousel({
        interval: 3000 //changes the speed
    })
    </script>

</body>
